{
  "dashboard": {
    "id": null,
    "title": "Cuenly - MÃ©tricas Extendidas (Firebase, OpenAI, Performance)",
    "tags": ["cuenly", "firebase", "openai", "performance", "extended"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 0,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Firebase Authentication Events",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cuenly_firebase_auth_events_total[5m])) by (event_type, success)",
            "legendFormat": "{{event_type}} - {{success}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "colors": [
          {"color": "green", "value": null},
          {"color": "red", "value": 1}
        ]
      },
      {
        "id": 2,
        "title": "Firebase Token Validation Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(cuenly_firebase_token_validation_duration_seconds_bucket[5m])) by (le, result))",
            "legendFormat": "P95 - {{result}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(cuenly_firebase_token_validation_duration_seconds_bucket[5m])) by (le, result))",
            "legendFormat": "P50 - {{result}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "OpenAI API Calls by Model",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cuenly_openai_api_calls_total[5m])) by (model, success)",
            "legendFormat": "{{model}} - {{success}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Calls/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "OpenAI Token Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cuenly_openai_tokens_used_total[5m])) by (model, token_type)",
            "legendFormat": "{{model}} - {{token_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Tokens/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Database Operations by Collection",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cuenly_database_operations_total[5m])) by (collection, operation_type)",
            "legendFormat": "{{collection}} - {{operation_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Operations/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Database Query Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(cuenly_database_query_duration_seconds_bucket[5m])) by (le, collection))",
            "legendFormat": "P95 - {{collection}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Trial Events",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cuenly_trial_events_total[5m])) by (event_type, user_type)",
            "legendFormat": "{{event_type}} - {{user_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Subscription Events",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cuenly_subscription_events_total[5m])) by (event_type, plan_type)",
            "legendFormat": "{{event_type}} - {{plan_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24}
      },
      {
        "id": 9,
        "title": "Security Events",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(cuenly_security_events_total[5m])) by (event_type, severity)",
            "legendFormat": "{{event_type}} - {{severity}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24},
        "alert": {
          "name": "High Security Events",
          "message": "High number of security events detected",
          "frequency": "10s",
          "conditions": [
            {
              "query": {
                "queryType": "",
                "refId": "A"
              },
              "reducer": {
                "type": "last",
                "params": []
              },
              "evaluator": {
                "params": [5],
                "type": "gt"
              }
            }
          ]
        }
      },
      {
        "id": 10,
        "title": "Email Processing Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(cuenly_email_processing_duration_seconds_bucket[5m])) by (le, user_type))",
            "legendFormat": "P95 - {{user_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 11,
        "title": "System Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "cuenly_system_resource_usage",
            "legendFormat": "{{resource_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Usage %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 12,
        "title": "Active Background Jobs",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(cuenly_background_jobs_active) by (job_type)",
            "legendFormat": "{{job_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 40}
      },
      {
        "id": 13,
        "title": "Frontend Performance Metrics",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"cuenly-frontend\"} |= \"web_vitals\" | json",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {"h": 8, "w": 16, "x": 8, "y": 40}
      },
      {
        "id": 14,
        "title": "User Session Analytics",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, count by (user_email) (rate({job=\"cuenly-frontend\"} |= \"session_started\" | json [24h])))",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 48}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "user_email",
          "type": "query",
          "query": "label_values({job=\"cuenly-backend\"}, user_email)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "user_type",
          "type": "query",
          "query": "label_values(cuenly_trial_events_total, user_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "expr": "resets(process_start_time_seconds[1h]) > 0",
          "titleFormat": "Deployment",
          "textFormat": "Application restarted"
        },
        {
          "name": "High Security Events",
          "datasource": "prometheus",
          "expr": "increase(cuenly_security_events_total[5m]) > 10",
          "titleFormat": "Security Alert",
          "textFormat": "High security event rate detected"
        }
      ]
    }
  },
  "overwrite": true
}